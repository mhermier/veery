
import "veery/compiler/abstract_lang/ast" for Expr

class ThisModuleExpr is Expr {
  construct new(keyword) {
    _keyword = keyword
  }

  keyword { _keyword }

  accept(visitor, visitor_data) { visitor.visitThisModuleExpr(this, visitor_data) }

  toString {
    return "ThisModule(%(_keyword))"
  }
}
